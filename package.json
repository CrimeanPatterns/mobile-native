  "scripts": {
    "postinstall": "patch-package --use-yarn && yarn jetify",
    "start": "yarn react-native start",
    "start:storybook": "cross-env ENVFILE=.env.storybook yarn react-native run-ios --configuration Debug",
    "start:ios": "cross-env ENVFILE=.env.development yarn react-native run-ios --simulator \"iPhone SE\"",
    "start:ios:ipad": "cross-env ENVFILE=.env.development yarn react-native run-ios --simulator \"iPad Air\"",
    "start:ios:release": "yarn react-native run-ios --simulator \"iPhone SE\" --configuration Release",
    "start:ios:device": "yarn react-native run-ios --device --configuration Release",
    "start:android": "cross-env ENVFILE=.env.development yarn react-native run-android",
    "start:android:release": "yarn react-native run-android --variant release",
    "test": "jest",
    "test:snapshot-update": "jest -u",
    "test:android": "TEST_PLATFORM=android jest --config jest.android.json",
    "test:android:snapshot-update": "yarn run test:android -u",
    "debugger": "which rndebugger-open && rndebugger-open || echo \"rndebugger-open not found\"",
    "clean-project": "yarn react-native clean-project-auto",
    "precommit": "lint-staged",
    "commit": "clear && git-cz",
    "ts-check": "yarn tsc --noEmit",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher"
  },
