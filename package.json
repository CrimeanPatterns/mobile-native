  "jest": {
    "preset": "react-native",
    "collectCoverage": false,
    "moduleNameMapper": {
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".*\\.android\\..*",
      "__utils__"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-*|@react-native-community|@react-navigation|react-navigation|react-navigation-.*|bugsnag|react-native-.*|rn-fetch-blob)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\\\.(js|ts|tsx)$": "babel-jest"
    },
    "snapshotResolver": "./jest.snapshot.resolver.js",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit",
      "post-checkout": "yarn && bundle exec pod install --project-directory=ios",
      "post-merge": "run-if-changed"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "yarn eslint --quiet"
    ]
  },
  "run-if-changed": {
    "yarn.lock": "yarn install",
    "ios/Podfile.lock": "bundle exec pod install --project-directory=ios"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=18"
  }
